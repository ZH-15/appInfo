<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="cn.app.dao.DataDictionaryMapper">
 	<select id="findappStatus" resultType="DataDictionary">
 		SELECT * FROM `data_dictionary`
		WHERE typeCode = "APP_STATUS"
 	</select>
 	
 	<select id="findappflatform" resultType="DataDictionary">	
		SELECT * FROM `data_dictionary`
		WHERE typeCode = "APP_FLATFORM"
 	</select>
 </mapper>